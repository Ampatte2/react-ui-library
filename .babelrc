{
    "presets": [
        "@babel/preset-env",
        "@babel/preset-react"
    ],
    "env": {
        "production": {
            "comments": false,
            "presets": [
                "@babel/preset-env",
                "@babel/preset-react",
                "minify"
            ],
            "ignore": [
                "**/__Layouts/*"
            ],
            "plugins": [
                [
                    "babel-plugin-styled-components",
                    {
                        "pure": true,
                        "minify": true
                    }
                ]
            ]
        }
    }
}
